    "/user/history": {
      "get": {
        "summary": "Get Search History",
        "description": "Get the authenticated user's search history.\n\nArgs:\n    user: Current user from JWT token\n    limit: Maximum number of searches to return (default: 20, max: 100)\n\nReturns:\n    User's recent searches sorted by timestamp (newest first)",
        "operationId": "get_search_history_user_history_get",
        "parameters": [
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "default": 20,
              "title": "Limit"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {

                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      },
      "delete": {
        "summary": "Delete Search History",
        "description": "Delete all search history for the authenticated user (GDPR compliance).\n\nArgs:\n    user: Current user from JWT token\n\nReturns:\n    Success status",
        "operationId": "delete_search_history_user_history_delete",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {

                }
              }
            }
          }
        }
      }
    },
    "/user/search": {
      "post": {
        "summary": "Save Search Alert",
        "description": "Save a search as an alert for the authenticated user.\n\nArgs:\n    req: SearchRequest object with search parameters\n    user: Current user from JWT token\n\nReturns:\n    Success confirmation with saved alert details",
        "operationId": "save_search_alert_user_search_post",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SearchRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {

                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      },
      "get": {
        "summary": "Get Search Alerts",
        "description": "Get the authenticated user's saved search alerts.\n\nArgs:\n    user: Current user from JWT token\n    limit: Maximum number of alerts to return (default: 20, max: 100)\n\nReturns:\n    User's saved search alerts sorted by timestamp (newest first)",
        "operationId": "get_search_alerts_user_search_get",
        "parameters": [
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "default": 20,
              "title": "Limit"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {

                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/user/delete": {
      "delete": {
        "summary": "Delete User",
        "description": "Delete the currently authenticated user's Cognito account.",
        "operationId": "delete_user_user_delete_delete",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DeleteUserResponse"
                }
              }
            }
          }
        }
      }
